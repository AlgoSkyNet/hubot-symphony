{
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:flowtype/recommended",
    "plugin:mocha/recommended"
  ],
  "plugins": [
    "flowtype",
    "mocha"
  ],
  "rules": {
    "max-len": [
      2,
      120,
      2
    ]
  }
}