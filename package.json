{
  "name": "hubot-symphony",
  "version": "1.0.0",
  "description": "Hubot adapter for Symphony",
  "main": "src/adapter.coffee",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register test/*.coffee",
    "test-cov": "mocha --compilers coffee:coffee-script/register --require blanket --reporter mocha-lcov-reporter test/*.coffee | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonfreedman/hubot-symphony.git"
  },
  "keywords": [
    "hubot",
    "adapter",
    "symphony"
  ],
  "author": "Jon Freedman",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jonfreedman/hubot-symphony/issues"
  },
  "homepage": "https://github.com/jonfreedman/hubot-symphony",
  "dependencies": {
    "fs": "0.0.2",
    "https": "^1.0.0",
    "log4js": "^0.6.37",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "coffee-script": ">=1.10.0",
    "coveralls": "^2.11.9",
    "hubot": ">=2.19",
    "mocha": ">=2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^8.0.0",
    "node-uuid": "^1.4.7"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
