{
  "name": "hubot-symphony",
  "description": "Hubot adapter for Symphony",
  "main": "src/adapter.coffee",
  "scripts": {
    "precommit": "npm test",
    "prepush": "npm test",
    "commit-msg": "validate-commit-msg",
    "diagnostic": "coffee src/diagnostic.coffee",
    "test": "mocha test/*.coffee",
    "test-cov": "cross-env COFFEECOV_INIT_ALL=false mocha --require coffee-coverage/register-istanbul test/*.coffee && istanbul report",
    "it": "mocha it/*.coffee",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "license-violations": "node-license-validator -c node-license-violations.json",
    "license-notice-warns": "node-license-validator -c node-license-notice-warns.json || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/symphonyoss/hubot-symphony.git"
  },
  "keywords": [
    "symphonyoss",
    "hubot",
    "adapter",
    "symphony"
  ],
  "author": "Jon Freedman",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/symphonyoss/hubot-symphony/issues"
  },
  "homepage": "https://github.com/symphonyoss/hubot-symphony",
  "dependencies": {
    "backoff": "2.5.0",
    "html-entities": "1.2.0",
    "log": "1.4.0",
    "memoizee": "0.4.1",
    "q": "1.4.1",
    "request": "2.79.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coffee-coverage": "1.0.1",
    "coffee-script": "1.12.2",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "cz-conventional-changelog": "1.2.0",
    "hubot": "2.19.0",
    "husky": "0.12.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nock": "9.0.2",
    "node-license-validator": "1.3.0",
    "semantic-release": "6.3.2",
    "uuid": "3.0.1",
    "validate-commit-msg": "2.8.2",
    "yargs": "6.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "This repo is Commitizen friendly, commit using git cz or see https://github.com/stevemao/conventional-changelog-angular/blob/master/convention.md"
    }
  }
}
